[user]
      # These values are set in ~/.gitconfig_local
[include]
  path = .gitconfig_local
[alias]
	co = checkout
	st = status
	ci = commit
[core]
	pager = delta
[interactive]
	diffFilter = delta --color-only
[delta]
	features = line-numbers decorations
[pull]
	rebase = false
[init]
	defaultBranch = main
