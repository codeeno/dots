
{
  "languageserver": {
    "terraform": {
      "command": "terraform-lsp",
      "filetypes": ["terraform"],
      "initializationOptions": {}
    }
  },
  "coc.preferences.formatOnSaveFiletypes": [
    "typescript",
    "javascript",
    "json",
    "rust",
    "python"
  ],
  "eslint.autoFixOnSave": true,
  "eslint.quiet": true,
  "eslint.options": {
    "useEslintrc": true
  }
}
